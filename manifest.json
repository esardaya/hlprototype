{
  "name": "HeuristicLab Prototype",
  "description": "Prototype for HeuristicLab",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "sandbox": {
    "pages": ["app/sandbox.html"]
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "16": "app/img/hl_icon_16.png",
    "48": "app/img/hl_icon_48.png",
    "128": "app/img/hl_icon_128.png"
  }
}