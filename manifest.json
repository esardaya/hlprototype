{
  "name": "HeuristicLab Prototype",
  "description": "Prototype for HeuristicLab",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>"
  ],
  "sandbox": {
    "pages": ["app/sandbox.html"]
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' 'unsafe-eval'",
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}